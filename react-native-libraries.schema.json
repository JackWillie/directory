{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "githubUrl"
    ],
    "properties": {
      "githubUrl": {
        "$id": "#/items/properties/githubUrl",
        "type": "string",
        "title": "The Githuburl Schema",
        "default": "",
        "examples": [
          "https://github.com/joshswan/react-native-autolink"
        ],
        "pattern": "^(.*)$"
      },
      "ios": {
        "$id": "#/items/properties/ios",
        "type": "boolean",
        "title": "The Ios Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "android": {
        "$id": "#/items/properties/android",
        "type": "boolean",
        "title": "The Android Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "expo": {
        "$id": "#/items/properties/expo",
        "type": "boolean",
        "title": "The Expo Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "examples": {
        "$id": "#/items/properties/examples",
        "type": "array",
        "title": "The Examples Schema",
        "default": null,
        "items": {
          "$id": "#/items/properties/examples/items",
          "type": "string",
          "title": "The Items Schema",
          "default": "",
          "examples": [
            "https://snack.expo.io/SkRP2Ehrb"
          ],
          "pattern": "^(.*)$"
        }
      },
      "npmPkg": {
        "$id": "#/items/properties/npmPkg",
        "type": "string",
        "title": "The Npmpkg Schema",
        "default": "",
        "examples": [
          "@expo/ex-navigation"
        ],
        "pattern": "^(.*)$"
      },
      "unmaintained": {
        "$id": "#/items/properties/unmaintained",
        "type": "boolean",
        "title": "The Unmaintained Schema",
        "default": false,
        "examples": [
          true
        ]
      },
      "thumbnails": {
        "$id": "#/items/properties/thumbnails",
        "type": "array",
        "title": "The Thumbnails Schema",
        "default": [null],
        "items": {
          "$id": "#/items/properties/thumbnails/items",
          "type": "string",
          "title": "The Thumbnails Item Schema",
          "default": "",
          "examples": [
            "https://reactnative.dev/img/homepage/phones.png"
          ],
          "pattern": "^(http(s?):)([/|.|\\w|\\s|-])*\\.(?:jpg|gif|png)$"
        }
      }
    }
  }
}